{
    "project_title": "MyAppName",
    "project_name": "{{ cookiecutter.project_title.lower().replace(' ','').replace('-','').replace('_','') }}",
    "project_repo": "{{ 'pyramid-app-' + cookiecutter.project_name }}",
    "project_slug": "{{ 'pyramid_app_' + cookiecutter.project_name }}",
    "project_short_description": "",
    "author_name": "Author",
    "author_email": "{{ cookiecutter.author_name.replace(' ','.').lower() + '@vanoord.com' }}",
    "inject_frontend": [
        "no",
        "yes"
    ],
    "_copy_without_render": [
        "*.pdf",
        "*.png",
        "*.jpg",
        "*.gif",
        "docs",
        "build",
        "htmlcov",
        "notebooks",
        ".eggs",
        ".mypy_cache",
        ".pytest_cache",
        "*__pycache__",
        "*.egg-info"
    ]
}